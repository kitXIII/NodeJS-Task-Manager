mixin flash(f, options = {})
  - const message = _.get(options, 'message', 'Do you have any problems?');
  - const type = _.get(options, 'type', 'info')
  .alert.alert-dismissible.fade.show(class=`alert-${type}`)= message
    button.close(type="button", data-dismiss="alert")
      span(aria-hidden="true") &times;